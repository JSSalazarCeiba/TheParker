/**
 * 
 */
package domain.model;

/**
 * @author juan.salazar
 *
 */
public class Plate {
	
	private String plateCode;
	private String type;
	private Boolean valid;
	
	/**
	 * @param plateCode
	 * @param type
	 * @param valid
	 */
	public Plate(String plateCode, String type, Boolean valid) {
		super();
		this.plateCode = plateCode;
		this.type = type;
		this.valid = valid;
	}

}
