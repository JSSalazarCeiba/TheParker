/**
 * 
 */
package testDataBuilder;

import java.util.Date;

import co.com.ceiba.theparker.domain.model.Vehicle;
import co.com.ceiba.theparker.domain.model.VehicleRecord;

/**
 * @author juan.salazar
 *
 */
public class VehicleRecordDataBuilder {

	private Long id;
	private Vehicle vehicle;
	private Integer cc;
	private Date timeIn;
	private Date timeOut;
	private Long fee;
	private String active;

	public VehicleRecordDataBuilder() {
		super();
		this.id = 10L;
		this.vehicle = new Vehicle("ABC123", "car");
		this.cc = null;
		this.timeIn = new Date();
		this.timeOut = null;
		this.fee = null;
		this.active = "YES";
	}

	public VehicleRecord build() {
		return new VehicleRecord(this.id, this.vehicle, this.cc, this.timeIn, this.timeOut, this.fee, this.active);
	}

}
