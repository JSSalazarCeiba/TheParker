package domain.model;

/**
 * @author juan.salazar
 *
 */
public class Response {
	
	private Integer code;
	private String message;
	/**
	 * @param code
	 * @param message
	 */
	public Response(Integer code, String message) {
		super();
		this.code = code;
		this.message = message;
	}

}
